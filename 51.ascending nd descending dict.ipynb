    "#SORT DICTIONARY IN ASCENDING AND DESCENDING ORDER\n",
    "a={'cherry':11,'grape':12,'apple':13,'mango':14}\n",
    "l=list(a.items())\n",
    "l.sort()\n",
    "print('ascending order is:',l)\n",
    "l=list(a.items())\n",
    "l.sort(reverse=True)\n",
    "print('descendind order is:',l)\n",
    "dict=dict(l)\n",
    "print(\"dictionary\",dict)"
